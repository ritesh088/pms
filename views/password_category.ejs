<%- include('header') %>

<link rel="stylesheet" href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css">


    <div class="container">
        <div class="row">
         
            <h1>Password Category List</h1>
            <br>
             <table id="passcattable" class="table table-striped table-primary">
            <thead>
                <tr>
                  <th> Password Category Name</th>
               
                  <th>Action</th>
                </tr>
            </thead>
            <tbody>
              <%if (records.length>0){ %>
                <% records.forEach(function(row) {%>
            <tr>
              
              <td><%= row.passord_category %></td>
				
              <td><a class="btn btn-primary btn-xs" href="/passwordCategory/edit/<%= row._id %>"><i class="fa fa-edit"></i></a>
                  <a class="btn btn-danger btn-xs" href="/passwordCategory/delete/<%= row._id %>"><i class="fa fa-trash"></i></a>
              </td>
            </tr>
           <% }) } %>
            </tbody>
           </table> 
    </div>
</div>

<script src="https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.21/js/dataTables.bootstrap4.min.js "></script>
<script>
  $(document).ready(function() {
    $('#passcattable').DataTable();
} );
</script>
  </body>
</html>
